//Client-side library management for CSS
{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "library": "jquery@3.4.1",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.4.1", //bootstrap version
      "destination": "wwwroot/lib/bootstrap/"
    }


  ]
}